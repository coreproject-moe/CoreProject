        "@tailwindcss/typography": "^0.5.4",
        "@typescript-eslint/eslint-plugin": "^5.32.0",
        "@typescript-eslint/parser": "^5.32.0",
        "autoprefixer": "^10.4.8",
        "cssnano": "^5.1.12",
        "daisyui": "^2.20.0",
        "easytimer.js": "^4.5.3",
        "eslint": "^8.21.0",
