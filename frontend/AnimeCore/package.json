{
    "name": "animecore",
    "version": "0.0.1",
    "scripts": {
        "dev": "vite dev --host",
        "build": "vite build",
        "build:static": "cross-env BUILD_STATIC_ENV=static vite build",
        "build:node": "cross-env BUILD_NODE_ENV=node vite build",
        "package": "svelte-kit package",
        "preview": "vite preview",
        "postinstall": "svelte-kit sync",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --check --plugin-search-dir=. .",
        "eslint": "eslint . --fix",
        "format": "prettier --write --plugin-search-dir=. .",
        "update": "npx npm-check-updates -u && npm install"
    },
    "devDependencies": {
        "@felte/reporter-tippy": "^1.1.5",
        "@felte/validator-yup": "^1.0.11",
        "@sveltejs/adapter-node": "^1.2.4",
        "@sveltejs/adapter-static": "^2.0.2",
        "@sveltejs/adapter-vercel": "^3.0.0",
        "@sveltejs/kit": "^1.20.1",
        "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
        "@tailwindcss/typography": "^0.5.9",
        "@types/js-cookie": "^3.0.3",
        "@types/uuid": "^9.0.1",
        "@types/voca": "^1.4.2",
        "@typescript-eslint/eslint-plugin": "^5.59.8",
        "@typescript-eslint/parser": "^5.59.8",
        "autoprefixer": "^10.4.14",
        "cross-env": "^7.0.3",
        "cssnano": "^6.0.1",
        "daisyui": "^3.0.1",
        "dayjs": "^1.11.8",
        "detect-browser": "^5.3.0",
        "easytimer.js": "^4.5.4",
        "eslint": "^8.41.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "eslint-plugin-svelte3": "^4.0.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "felte": "^1.2.7",
        "js-cookie": "^3.0.5",
        "postcss": "^8.4.24",
        "postcss-load-config": "4.0.1",
        "postcss-preset-env": "^8.4.2",
        "prettier": "^2.8.8",
        "prettier-plugin-svelte": "^2.10.1",
        "prettier-plugin-tailwindcss": "^0.3.0",
        "sass": "^1.62.1",
        "svelte": "^3.59.1",
        "svelte-check": "^3.4.3",
        "svelte-gestures": "^1.5.2",
        "svelte-preprocess": "^5.0.3",
        "svelte-star-rating": "^2.0.1",
        "swiper": "^8.4.7",
        "tailwind-scrollbar-hide": "^1.1.7",
        "tailwindcss": "^3.3.2",
        "tslib": "^2.5.2",
        "typescript": "^5.1.3",
        "uuid": "^9.0.0",
        "vite": "4.3.9",
        "voca": "^1.4.1"
    },
    "type": "module"
}
